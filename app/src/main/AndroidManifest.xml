<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.gramo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Context.GRAMOApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GRAMO"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme"
        tools:targetApi="n">
        <activity android:name="com.example.gramoproject.activity.sign.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.gramoproject.activity.MainActivity"/>
        <activity android:name="com.example.gramoproject.activity.sign.RegisterActivity" />
        <activity android:name="com.example.gramoproject.activity.notice.NoticeActivity"/>
        <activity android:name="com.example.gramoproject.activity.notice.NoticeAddActivity" />
        <activity android:name="com.example.gramoproject.activity.calendar.CalendarActivity" />
        <activity android:name="com.example.gramoproject.activity.homework.HomeworkAddActivity" />
        <activity android:name="com.example.gramoproject.activity.homework.HomeworkShowActivity" />
        <activity android:name="com.example.gramoproject.activity.homework.HomeworkMainActivity" />

    </application>

</manifest>